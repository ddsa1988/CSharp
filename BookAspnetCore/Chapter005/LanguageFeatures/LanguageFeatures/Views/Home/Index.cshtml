@model IEnumerable<string>

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Language Features</title>
</head>
<body>
    <ul>
        @foreach (string content in Model) {
            <li>@content</li>
        }
    </ul>
</body>
</html>